<%- contentFor('body') %>
<h3>> Vitrine</h3>

<div class="row">
    <% systemList.map(system => { %>
    <div class="col-md-6 system-block">
        <div class="system-title">
            <span><%= system.name %></span>
        </div>
        <iframe src="<%= system.embed %>" class="responsive-iframe" frameborder="0" style="border:0;" allowfullscreen=""
            aria-hidden="false" tabindex="0"></iframe>
        <hr>
    </div>
    <% }); %>

</div>